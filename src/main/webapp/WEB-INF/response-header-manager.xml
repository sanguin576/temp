	<mapping url="/">
		<default>
			<response-headers>
				<header key="X-Frame-Options" value="SAMEORIGIN" />
				<header key="X-Content-Type-Options" value="nosniff" />
				<header key="X-XSS-Protection" value="1" />
			</response-headers>
		</default>
	</mapping>
</response-header-mapper>